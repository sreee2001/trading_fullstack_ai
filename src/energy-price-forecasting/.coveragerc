# Coverage Configuration for Energy Price Forecasting System

[run]
source = .
omit =
    */tests/*
    */venv/*
    */__pycache__/*
    */mlruns/*
    */examples/*
    */docs/*
    setup.py
    */migrations/*
    */node_modules/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @property
    @abc.abstractmethod

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov

[xml]
output = coverage.xml

